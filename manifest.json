{
    "manifest_version": 3,
    "name": "YouTube Hider",
    "version": "1.0",
    "description": "Hide specific elements on YouTube pages.",
    "host_permissions": ["*://*.youtube.com/*"],
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ]
  }
  